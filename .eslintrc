{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-param-reassign": 0,
    "no-throw-literal": 0,
    "arrow-body-style": 0,
    "react/forbid-prop-types": 0,
    "comma-dangle": ["error", "never"],
    "no-underscore-dangle": ["error", { "allow": ["_embedded", "_links", "_etag"] }],
    "linebreak-style": 0
  }
}
