        "dct:modified": {
          "default": "2018-04-19",
          "description":
            "De datum waarop deze beschrijving van de gegevensset voor het laatst is gewijzigd",
          "format": "date",
          "minLength": 1,
          "pattern":
            "^\\d\\d\\d\\d-[01]\\d-[0-3]\\d(?:T[012]\\d:[0-5]\\d:[0-5]\\d(?:\\.\\d+)?)?(?:Z|[01]\\d(?::[0-5]\\d)?)?$",
