      "type": "string"
    },
    "ams:serviceType": {
      "description": "Geef het type API of webservice",
      "enum": [
        "atom",
        "rest",
        "csw",
        "wcs",
        "wfs",
        "wms",
        "wmts",
        "soap",
        "other"
      ],
      "enumNames": [
        "REST: Atom feed",
        "REST: overig",
        "CSW",
        "WCS",
        "WFS",
        "WMS",
        "WMTS",
        "SOAP",
        "Anders"
