        },
        "vcard:hasEmail": {
          "format": "email",
          "title": "E-mail inhoudelijk contactpersoon",
          "type": "string"
        },
        "vcard:hasURL": {
          "format": "uri",
          "title": "Website inhoudelijk contactpersoon",
          "type": "string"
        }
      },
      "required": [
        "vcard:fn"
      ],
      "title": "",
      "type": "object"
    },
    "dcat:distribution": {
